      <span class="icon" aria-hidden="true"><svg xmlns="http://www.w3.org/2000/svg" width="1228" height="1228" viewBox="0 0 1228 1228" fill="none"><circle r="614" cx="614" cy="614" fill="black"/><path d="M714.163 519.284L1160.89 0H1055.03L667.137 450.887L357.328 0H0L468.492 681.821L0 1226.37H105.866L515.491 750.218L842.672 1226.37H1200L714.137 519.284H714.163ZM569.165 687.828L521.697 619.934L144.011 79.6944H306.615L611.412 515.685L658.88 583.579L1055.08 1150.3H892.476L569.165 687.854V687.828Z" fill="white" style="transform: scale(.55);transform-origin: center;"/></svg></span>
    {%- endunless -%}
  </a>
{%- endif -%}

{%- if settings.social_custom != '' -%}
  <a href="{{ settings.social_custom | escape }}" class="{{ link_class }}" target="_blank">
    {%- if type == 'link' and hide_icon == true -%}
      <span class="link-arrow__icon">{%- render 'theme-symbols', icon: 'arrow' -%}</span>
      <span class="link-arrow__text"><span class="{{ text_class }}">{{ settings.social_custom_label | escape }}</span></span>
    {%- else -%}
      <span class="{{ text_class }}">{{ settings.social_custom_label | escape }}</span>
    {%- endif -%}

    {%- unless hide_icon -%}
      <span class="icon" aria-hidden="true">
        <img src="{{ settings.social_custom_icon | image_url: width: 24, height: 24, crop: 'center' }}" srcset="{{ settings.social_custom_icon | image_url: width: 24, height: 24, crop: 'center' }} 24w, {{ settings.social_custom_icon | image_url: width: 48, height: 48, crop: 'center' }} 48w, {{ settings.social_custom_icon | image_url: width: 72, height: 72, crop: 'center' }} 72w" sizes="24px" width="{{ settings.social_custom_icon.width }}" height="{{ settings.social_custom_icon.height }}" style="width:100%;height:100%" alt="{{ settings.social_custom_label | escape }}" />
      </span>
    {%- endunless -%}
  </a>
{%- endif -%}